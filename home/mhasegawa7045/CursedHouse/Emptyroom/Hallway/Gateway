#!/bin/bash
PATH=/bin:/usr/bin:

NONE='\033[00m'
RED='\033[01;31m'
GREEN='\033[01;32m'
YELLOW='\033[01;33m'
PURPLE='\033[01;35m'
CYAN='\033[01;36m'
WHITE='\033[01;37m'
BOLD='\033[1m'
UNDERLINE='\033[4m'
echo "You see a strange portal that has a sign that says Freedom"
echo "If you want to enter...Use the pinpad to type the code"
echo -e "${CYAN}--------"
echo "|1 2 3 |
|4 5 6 |
|7 8 9 |
|  0   |
--------"
echo -e "${NONE}"
read PASS
if [ $PASS != "1134" ]; then
echo "Wrong Passcode"
else
echo -e "${RED}Congratulations You found the passcode!"
echo -e "As a form of congratulations, would you like me to reveal myself? y/n${NONE}"
read RESP
